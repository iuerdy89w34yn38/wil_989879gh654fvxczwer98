Options +FollowSymLinks 

RewriteEngine On



# Rewrite for index.php,cars.php,...

#RewriteRule ^(home)$ index.php?page_name=$1
#RewriteRule ^(index)$ index.php?page_name=$1
#RewriteRule ^(projects)$ project.php?page_name=$1



# Rewrite for single variables

RewriteRule ^about-([^/]+)$ about.php?page=$1
RewriteRule ^web-([^/]+)$ web.php?page=$1
RewriteRule ^dprojects-([^/]+)$ dproject.php?dproject_name=$1
RewriteRule ^services-([^/]+)$ service.php?service_name=$1
RewriteRule ^dservices-([^/]+)$ dservice.php?dservice_name=$1
RewriteRule ^clients-([^/]+)$ client.php?client_name=$1
RewriteRule ^gallery-([^/]+)$ gallary.php?gallery_name=$1
RewriteRule ^pages-([^/]+)$ page.php?pages_name=$1
RewriteRule ^cpages-([^/]+)$ cpage.php?cpage_name=$1



